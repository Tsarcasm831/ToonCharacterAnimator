{
  "name": "toon-character-animator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:public": "concurrently -k \"vite --host 0.0.0.0 --port 3000\" \"npx -y cloudflared tunnel --url http://localhost:3000\"",
    "build": "tsc --noEmit && vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "nipplejs": "^0.10.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.19.7",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "terser": "^5.46.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^3.1.6",
    "vitest": "^3.2.4"
  }
}
